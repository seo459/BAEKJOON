#include <iostream>
using namespace std;

void swap(int** arr, int idx_1, int idx_2)
{
	int tmp_x = arr[idx_1][0], tmp_y = arr[idx_1][1];
	arr[idx_1][0] = arr[idx_2][0]; arr[idx_1][1] = arr[idx_2][1];
	arr[idx_2][0] = tmp_x; arr[idx_2][1] = tmp_y;
}
void non_recursive_quicksort(int** arr, int left, int right, char ch)
{
	int s = 0;
	int stack[128];
	stack[s++] = left;
	stack[s++] = right;
	while (s > 0)
	{
		if (left >= right) return;
		left = stack[s--];
		right = stack[s--];
		int pivot = left;
		int low = left + 1, high = right;
		while (low < high) {
			while (arr[low][0] < arr[pivot][0] && low < high)
				low++;
			while (arr[high][0] > arr[pivot][0] && low < high)
				high--;
			swap(arr, low, high);
		}
		swap(arr, pivot, high);
		stack[s++] = high + 1; //new left
		stack[s++] = high - 1; //new right
	}
}
int main()
{
	int N;
	cin >> N;
	int** arr = new int* [N];
	for (int i = 0; i < N; i++)
		arr[i] = new int[2];
	//N*2 dynamic allocation
	for (int i = 0; i < N; i++)
	{
		cin >> arr[i][0] >> arr[i][1];
	}	

}
